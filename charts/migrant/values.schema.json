{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "database_credentials",
        "image",
        "migrations"
    ],
    "properties": {
        "database_credentials": {
            "type": "object",
            "default": {},
            "title": "The database_credentials Schema",
            "required": [],
            "properties": {
                "env": {
                    "type": "object",
                    "default": {},
                    "title": "The env Schema",
                    "required": [],
                    "examples": [
                        {
                            "DB_HOST": "postgresql-postgresql.devtroncd",
                            "DB_PASSWORD": "",
                            "DB_PORT": "1234",
                            "DB_TYPE": "postgres"
                        }
                    ]
                },
                "envFrom": {
                    "type": "object",
                    "default": {},
                    "title": "The envFrom Schema",
                    "required": [],
                    "properties": {
                        "secret": {
                            "type": "array",
                            "default": [],
                            "title": "The secret Schema",
                            "items": {
                                "type": "string",
                                "title": "A Schema",
                                "examples": [
                                    "db-secret"
                                ]
                            },
                            "examples": [
                                [
                                    "db-secret"
                                ]
                            ]
                        }
                    },
                    "examples": [
                        {
                            "secret": [
                                "db-secret"
                            ]
                        }
                    ]
                }
            },
            "examples": [
                {
                    "env": {
                        "DB_HOST": "postgresql-postgresql.devtroncd",
                        "DB_PASSWORD": "",
                        "DB_PORT": "1234",
                        "DB_TYPE": "postgres"
                    },
                    "envFrom": {
                        "secret": [
                            "db-secret"
                        ]
                    }
                }
            ]
        },
        "image": {
            "type": "string",
            "default": "migrate/migrate",
            "title": "The image Schema",
            "examples": [
                "migrate/migrate"
            ]
        },
        "migrations": {
            "type": "array",
            "default": [],
            "title": "The migrations Schema",
            "items": {
                "type": "object",
                "default": {},
                "title": "A Schema",
                "required": [
                    "enable",
                    "env"
                ],
                "properties": {
                    "enable": {
                        "type": "boolean",
                        "default": false,
                        "title": "The enable Schema",
                        "examples": [
                            true
                        ]
                    },
                    "env": {
                        "type": "object",
                        "default": {},
                        "title": "The env Schema",
                        "required": [
                            "REPO_URL",
                            "REPO_NAME",
                            "DB_NAME"
                        ],
                        "examples": [
                            {
                                "REPO_URL": "https://github.com/devtron-labs/devtron",
                                "REPO_NAME": "devtron",
                                "BRANCH": "main",
                                "COMMIT": "",
                                "SCRIPT_LOCATION": "scripts/sql/",
                                "PARAM": "sslmode=disable",
                                "MIGRATE_TO_VERSION": "0",
                                "DB_NAME": "orchestrator"
                            }
                        ]
                    },
                    "post_query": {
                        "type": "array",
                        "default": [],
                        "title": "The post_query Schema",
                        "items": {},
                        "examples": [
                            ["ls"]
                        ]
                    }
                },
                "examples": [
                    {
                        "enable": true,
                        "env": {
                            "REPO_URL": "https://github.com/devtron-labs/devtron",
                            "REPO_NAME": "devtron",
                            "BRANCH": "main",
                            "COMMIT": "",
                            "SCRIPT_LOCATION": "scripts/sql/",
                            "PARAM": "sslmode=disable",
                            "MIGRATE_TO_VERSION": "0",
                            "DB_NAME": "orchestrator"
                        },
                        "post_query": []
                    }
                ]
            },
            "examples": [
                [
                    {
                        "enable": true,
                        "env": {
                            "REPO_URL": "https://github.com/devtron-labs/devtron",
                            "REPO_NAME": "devtron",
                            "BRANCH": "main",
                            "COMMIT": "",
                            "SCRIPT_LOCATION": "scripts/sql/",
                            "PARAM": "sslmode=disable",
                            "MIGRATE_TO_VERSION": "0",
                            "DB_NAME": "orchestrator"
                        },
                        "post_query": []
                    }
                ]
            ]
        }
    },
    "examples": [
        {
            "database_credentials": {
                "env": {
                    "DB_HOST": "postgresql-postgresql.devtroncd",
                    "DB_PASSWORD": "",
                    "DB_PORT": "1234",
                    "DB_TYPE": "postgres"
                },
                "envFrom": {
                    "secret": [
                        "db-secret"
                    ]
                }
            },
            "image": "migrate/migrate",
            "migrations": [
                {
                    "enable": true,
                    "env": {
                        "REPO_URL": "https://github.com/devtron-labs/devtron",
                        "REPO_NAME": "devtron",
                        "BRANCH": "main",
                        "COMMIT": "",
                        "SCRIPT_LOCATION": "scripts/sql/",
                        "PARAM": "sslmode=disable",
                        "MIGRATE_TO_VERSION": "0",
                        "DB_NAME": "orchestrator"
                    },
                    "post_query": []
                }
            ]
        }
    ]
}